# Production Environment Configuration for convertcast.app
NODE_ENV=production
MOCK_DATABASE=false
ENABLE_MOCK_FEATURES=false

# Application URLs
NEXT_PUBLIC_APP_URL=https://convertcast.app

# Supabase Configuration (Production)
NEXT_PUBLIC_SUPABASE_URL=your_production_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_production_supabase_service_role_key

# Mux Configuration (Production)
MUX_TOKEN_ID=your_production_mux_token_id
MUX_TOKEN_SECRET=your_production_mux_token_secret
NEXT_PUBLIC_MUX_ENV_KEY=your_production_mux_env_key
NEXT_PUBLIC_MUX_CONFIGURED=true

# WebSocket Configuration (Production)
NEXT_PUBLIC_WEBSOCKET_URL=wss://ws.convertcast.app

# Additional Production Settings
NEXTAUTH_URL=https://convertcast.app
NEXTAUTH_SECRET=your_production_nextauth_secret